	Font.Draw ("a) The Fablulous Baker Boys", 115, 75, font7, 0)
	Font.Draw ("b) The Blues Brothers", 395, 75, font7, 0)
	Font.Draw ("c) The Brothers McMullen", 115, 25, font7, 0)
